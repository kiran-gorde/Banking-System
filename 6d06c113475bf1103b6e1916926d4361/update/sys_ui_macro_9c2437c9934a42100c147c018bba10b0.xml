        <xml><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
<a><img src="http://pluspng.com/img-png/whatsapp-hd-png-whatsapp-logo-png-1000.png" height="25" width="25" onclick="userwindow('${ref}')" tabindex="0"/></a>
<script>
function userwindow(reference)
{
	var caller_ref=reference.split(".");
	var fieldName=caller_ref[1];
	var _mobNo;
	var demoText;
	var number=g_form.getValue('number');
	var category=g_form.getValue('category');
	var sysId=g_form.getValue(fieldName);
	var user=new GlideRecord('sys_user');
	if(user.get(sysId))
	{
		_mobNo=user.mobile_phone.toString();
		demotext="Number:"+number+"\nCategory"+category;
	}
	mob=_mobNo.replace(/[^0-9]+/gi,"");
	if(mob)
	{
		open('https://wa.me/'+_mobNo+'?text='+encodeURIComponent(demoText),'_top');

	}
	else
	{
		g_from.addErrorMessage('mobile number  is not valid');
	}
}
</script>
</j:jelly>]]></xml>
